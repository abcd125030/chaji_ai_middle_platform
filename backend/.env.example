# Django Core
DJANGO_SECRET_KEY=your-secret-key-change-in-production
DJANGO_DEBUG=True
IS_TEST_ENV=False
DJANGO_ALLOWED_HOSTS=localhost,127.0.0.1,aigc.bwcj.biz
DJANGO_CSRF_TRUSTED_ORIGINS=http://localhost:8080,http://127.0.0.1:8080,http://localhost:3000,http://127.0.0.1:3000,http://aigc.bwcj.biz
CORS_ALLOWED_ORIGINS=http://localhost:8080,http://127.0.0.1:8080,http://localhost:3000,http://127.0.0.1:3000,http://aigc.bwcj.biz

# PostgreSQL 数据库设置
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=your-database-password
DB_DATABASE=X
USE_PGBOUNCER=True

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your-redis-password
CELERY_DB=0
CACHE_DB=1
CELERY_RESULT_DB=2

# Feishu
FEISHU_APP_ID=your-feishu-app-id
FEISHU_APP_SECRET=your-feishu-app-secret

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Server
SERVER_HOST=http://localhost:8000
SERVER_PROTOCOL=http

# 决定是否从 Redis 缓存加载LLM 配置（如果模型配置修改较为频繁，不宜开启）
LOAD_LLM_FROM_REDIS=False

# 图节点默认模型
# DEFAULT_MODEL=moonshotai/kimi-k2-instruct
# DEFAULT_MODEL=openai/gpt-oss-120b  # 这是 model_id
DEFAULT_MODEL=Groq GPT OSS 120b  # 使用数据库中的 name 字段

# Branding SeedEdit-3.0-i2i
DOUBAO_API_KEY=your-doubao-api-key
VOLC_ACCESS_KEY=your-volc-access-key
VOLC_SECRET_KEY=your-volc-secret-key-base64-encoded
CELERY_WORKER_CONCURRENCY=5
CELERY_NUM_WORKERS=2

# User Activation Settings
# 当设置为True时,通过邮箱注册/Google授权登录的用户默认为未激活状态,需要管理员手动激活
# 飞书登录的企业用户不受影响,仍默认为激活状态
REQUIRE_USER_ACTIVATION=true

# 服务于nginx反代
# FORCE_SCRIPT_NAME=/_X

# Web API 回调配置
WEB_API_BASE_URL=http://localhost:3000/_studio/api
INTERNAL_API_TOKEN=default-internal-token-change-in-production

# image editor生图提示词拼接逻辑,True代表风格提示词在前
STYLE_PROMPT_FIRST=True

# Qdrant API KEY
QDRANT_API_KEY=test-api-key

# 是否启用OSS
MINERU_USE_OSS=true

# 通知邮件所需的功能
EMAIL_HOST=smtp.exmail.qq.com
EMAIL_PORT=465
EMAIL_USE_SSL=True
EMAIL_USE_TLS=False
EMAIL_HOST_USER=info@frago.ai
EMAIL_HOST_PASSWORD=your-email-password
DEFAULT_FROM_EMAIL=info@frago.ai
SITE_NAME=frago.ai

# 启用或禁用planner的链式架构
ENABLE_PLANNER_CHAIN=false

# 内部业务专用
# qwen3-vl-plus 视觉模型分割pdf专用
DASHSCOPE_API_KEY=api-key

# pdf_extractor相关（生产环境下的图片域名，开发环境下不需要配置）
OSS_PUBLIC_DOMAIN=
DJANGO_OCR_API_URL=https://your-domain.com/_X/api/toolkit/ocr